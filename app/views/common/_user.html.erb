<% if current_user.present?%>
<div class="col-md-12">
<div class="media">
  <a class="pull-left" href="#">
    <div class="user_image">
    <%= image_tag current_user_info.try(:get_user_image)%>
    </div>
  </a>
  <div class="media-body">
    <h4 class="media-heading">
      <%= current_user_info.try(:nick_name)%>
    </h4><p>
    <%= current_user_info.try(:sign)%><br /> 28 关注| 26 粉丝
  </p>
  </div>
</div>
</div>
<% end %>